<%= link_to "Add a trigger", triggers_new_path %>
<br />
<br />
<% @triggers.each do |t| %>
	<%= t['function'] %>
	<%= t['input'] %>
	<% t['streams'].each do |s| %>
		<%= s %>
	<% end %>
	<%= link_to "Delete", {controller: "triggers", action: "destroy", query: t.to_json}, {method: :delete} %>
	<br />
<% end %>