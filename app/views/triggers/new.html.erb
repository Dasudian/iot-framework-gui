<%= form_for(@trigger, :url => {:action => "create"}) do |f| %>
	
	<%= f.label :function %>
	<%= f.select :function, ['greater_than', 'less_than'] %>

	<%= f.label :input %>
	<%= f.text_field :input %>

	<%= f.label  :streams %>
	<%= f.select :streams, @stream_ids %>

	<%= f.submit "Submit" %>
<% end %>